"use strict";
function texto() {
    var x;

    x = document.getElementById("text").value;
   
    document.getElementById("demo").innerHTML = x;


}
function texto1() {
    var y;

    y = document.getElementById("text1").value;
   
    document.getElementById("atributo").innerHTML = y;


}
function texto2() {
    var z;

    z = document.getElementById("text2").value;
   
    document.getElementById("operando").innerHTML = z;


}
